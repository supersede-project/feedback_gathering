<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">

    <ImageView
        android:id="@+id/supersede_feedbacklibrary_sticker_row_layout_icon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="@dimen/supersede_feedbacklibrary_sticker_row_layout_margin"
        android:layout_marginStart="@dimen/supersede_feedbacklibrary_sticker_row_layout_margin"
        android:layout_marginTop="@dimen/supersede_feedbacklibrary_sticker_row_layout_margin">
    </ImageView>

    <TextView
        android:id="@+id/supersede_feedbacklibrary_sticker_row_layout_label"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_vertical"
        android:layout_marginStart="@dimen/supersede_feedbacklibrary_sticker_row_layout_margin"
        android:textSize="12sp">
    </TextView>

</LinearLayout>